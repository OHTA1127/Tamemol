<h1>編集</h1>
<%= form_with model:@user, local: true do |f| %>
  <h4>Name</h4>
  <%= f.text_field :name %>

  <h4>プロフィール画像</h4>
  <%= f.attachment_field :profile_image %>

  <h4>目標</h4>
  <%= f.text_area :goal_comment %>

  <h4>目標金額</h4>
  <%= f.text_field :goal_money %>

  <%= f.submit '更新' %>
<% end %>